{
  "name": "stencil-shared-css",
  "version": "1.0.0",
  "description": "Share css files if multiple instances of a web component are insantiated",
  "bin": {
    "deduplicate": "./bin/deduplicate"
  },
  "scripts": {
    "build": "npm run build:es2015 && npm run build:esm && npm run build:cjs",
    "build:es2015": "tsc --module es2015 --target es2015 --outDir dist/es2015",
    "build:esm": "tsc --module es2015 --target es5 --outDir dist/esm",
    "build:cjs": "tsc --module commonjs --target es5 --outDir dist/cjs"
  },
  "author": "Jorge Paravicini",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/es2015/index.js",
  "es2017": "dist/esm/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jorgeparavicini/stencil-shared-css.git"
  },
  "bugs": {
    "url": "https://github.com/jorgeparavicini/stencil-shared-css/issues"
  },
  "keywords": [
    "stencil",
    "shared",
    "css"
  ],
  "devDependencies": {
    "eslint": "^7.28.0",
    "prettier": "2.3.1",
    "typescript": "^4.3.2",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "rollup": "^2.8.2",
    "uglifyjs": "^2.4.11"
  },
  "dependencies": {
    "glob": "^7.1.7"
  },
  "files": [
    "/dist/"
  ]
}
